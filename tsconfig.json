{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"types": ["astro/client"],
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@data/*": ["src/data/*"],
			"@layouts/*": ["src/layouts/*"],
			"@pages/*": ["src/pages/*"],
			"@styles/*": ["src/styles/*"],
			"@utils/*": ["src/utils/*"]
		}
	}
}
